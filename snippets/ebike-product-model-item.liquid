{%- elsif pdptype == 'customer_service' -%}
    <div class="ebike-customer-service">
        <div class="friab-buy-customer" >
            {%- if block.settings.customer_img -%}
                {% assign handsimg = block.settings.customer_img | img_url: '' %}
            {%- else -%}
            {% assign handsimg = 'https://cdn.shopify.com/s/files/1/0592/3766/2905/files/image_1.png?v=1667636453' %}
            {%- endif -%}
            <img
                class="Image--lazyLoad Image--fadeIn friab-customer-img"
                data-src="{{ handsimg }}"
                style="border-radius: 50%;"
                onclick="TidioShowFun()"
            >
            <div class="abcustomer_service_right" onclick="TidioShowFun()">
                <p> {{ block.settings.customer_title }} </p>
                <p> {{ block.settings.customer_desc }} </p>
            </div>
        </div>
    </div>
    <style>
        .ebike-customer-service{
            width: 100%;
        }
        .ebike-customer-service p {
            margin: 0;
            padding: 0;
        }
        @media only screen and (min-width: 769px) { 
            /* pc */
            .friab-buy-customer{
                width: 100%;
                padding-top: 24px;
                padding-bottom: 24px;
                display: flex;
                align-items: center;
                justify-content: flex-start;
                border-bottom: 1px solid #e6e6e6;
            }
            .friab-customer-img{
                width: 40px;
                height: 40px;
