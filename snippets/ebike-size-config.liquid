{% if type == 'item' %}
  <div class="ebike-size-ul">
    {%- assign sizeconfig_index = 1 -%}
    {%- for block in section.blocks -%}
      {%- case block.type -%}
        {%- when 'size_fit_table' -%}
          <div
            class="ebike-size-li fb-flex-x-between w-full cursor-pointer border-b border-e6e6e6 py-[20px] text-left text-h-14 font-medium text-121212 fb-769:py-6 fb-769:text-h-16 fb-769:font-semibold {% if template contains 'ebike' %}js-open-ebikedilaog{% else %}js-open-litboomdialog{% endif %}"
            data-name="{{ block.settings.size_fit_title | strip_html }}"
            data-img="{{ block.settings.size_product_img | img_url: '' }}"
            data-index="{{ sizeconfig_index }}"
            aria-expanded="false"
            data-action="open-modal"
            aria-controls="ebike-sizeli-dialog"
            aria-label="Open OK"
          >
            <span class="cursor-pointer font-semibold">{{ block.settings.size_fit_title }} </span>
            {% if template contains 'ebike' %}
              <span class="span-svg h-[14px] w-3.5 fb-769:h-5 fb-769:w-5">
                <img
                  loading="lazy"
                  class="h-auto w-full"
                  alt="plus logo"
                  src="https://cdn.shopify.com/s/files/1/0592/3766/2905/files/open-line-item.png?v=1689144600"
                >
              </span>
            {% else %}
              <svg
                class="span-svg2 mr-2 h-[14px] w-3.5 fb-769:h-5 fb-769:w-5"
                width="20"
                height="20"
                viewBox="0 0 20 20"
                fill="none"
                xmlns="http://www.w3.org/2000/svg"
              >
                <g id="Frame">
                    <path id="Vector" d="M7.22054 5.66996C7.3719 5.54736 7.56076 5.48047 7.75554 5.48047C7.95032 5.48047 8.13919 5.54736 8.29054 5.66996L12.7905 9.52996C12.8576 9.58531 12.9116 9.65481 12.9487 9.73347C12.9857 9.81212 13.0049 9.898 13.0049 9.98496C13.0049 10.0719 12.9857 10.1578 12.9487 10.2365C12.9116 10.3151 12.8576 10.3846 12.7905 10.44L8.29054 14.3C8.13919 14.4226 7.95032 14.4894 7.75554 14.4894C7.56076 14.4894 7.3719 14.4226 7.22054 14.3C7.15348 14.2446 7.09949 14.1751 7.06243 14.0964C7.02536 14.0178 7.00614 13.9319 7.00614 13.845C7.00614 13.758 7.02536 13.6721 7.06243 13.5935C7.09949 13.5148 7.15348 13.4453 7.22054 13.39L11.2205 9.98996L7.22054 6.58996C7.1517 6.53467 7.09615 6.46464 7.05798 6.38503C7.01982 6.30541 7 6.21825 7 6.12996C7 6.04167 7.01982 5.9545 7.05798 5.87489C7.09615 5.79528 7.1517 5.72524 7.22054 5.66996Z" fill="#121212" fill-opacity="0.5"/>
                </g>
              </svg>
            {% endif %}
            {% comment %} ebike专属 {% endcomment %}
            <div class="size-fit-config-desc-hide" style="display: none;">
              {{ block.settings.size_fit_desc }}
            </div>
          </div>
          {%- assign sizeconfig_index = sizeconfig_index | plus: 1 -%}
      {%- endcase -%}
    {%- endfor -%}
  </div>
{%- elsif type == 'dialog' -%}
