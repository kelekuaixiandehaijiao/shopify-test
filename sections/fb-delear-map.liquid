<div id="section-{{ section.id }}" class="delear-map-container">
  <div class="delear-map-bigtitle">{{ section.settings.delear_bigtitle1 }}</div>
  <div class="delear-tabs">
    <div class="delear-tabs-title delear-selected-title" data-delear-status="0">{{ section.settings.delear_tab_title1 }}</div>
    <div class="delear-tabs-title" data-delear-status="1">{{ section.settings.delear_tab_title2 }}</div>
  </div>
  <div class="delear-map-main">
    <div class="delear-desc">{{ section.settings.delear_desc }}</div>
    <div class="delear-network-content">
      <div class="delear-network-image">
        {% assign delear-network-icon = section.settings.delear_network_icon | img_url: '' %}
        <img
          class="Image--lazyLoad Image--fadeIn delear-network-icon"
          data-src="{{ delear-network-icon }}"
          data-sizes="auto"
          alt="{{ section.settings.delear_network_icon.alt | escape}}"
        >
      </div>
      <div class="delear-network-info">{{ section.settings.delear_network_title }}</div>
    </div>
    <div class="delear-map-content" id="beeline-locator"></div>
    <div class="delear-map-bigtitle">{{ section.settings.delear_bigtitle2 }}</div>
    <div class="delear-info">{{ section.settings.delear_info }}</div>
    <a href="{{ section.settings.delear_link }}">
      <div class="deleat-link">
        {{ section.settings.delear_link_text }}
      </div>
    </a>
    <div class="choose-delear">
      <div class="choose-delear-image">
        {% assign delear-map-pcimg = section.settings.delear-map-pcimg | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
        {% assign delear-map-mobimg = section.settings.delear-map-mobimg | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
        {% comment %} <img
          class="Image--lazyLoad Image--fadeIn delear-map-pcimg"
          data-src="{{ delear-map-pcimg }}"
          data-sizes="auto"
          data-optimumx="1.2"
          data-widths="[400, 600, 800, 1000, 1200, 1400, 1600, 1800, 2000, 2200]"
          alt="{{ section.settings.delear-map-pcimg.alt | escape }}"
        > {% endcomment %}
        {% comment %} <img
          class="Image--lazyLoad Image--fadeIn delear-map-mobimg"
          data-src="{{ delear-map-mobimg }}"
          data-sizes="auto"
          data-optimumx="1.2"
          data-widths="[400, 600, 800, 1000, 1200, 1400, 1600, 1800, 2000, 2200]"
          alt="{{ section.settings.delear-map-mobimg.alt | escape }}"
        > {% endcomment %}
      </div>
      <div class="choose-delear-content">
        <div class="choose-delear-wrapper">
          <div class="choose-delear-title">{{ section.settings.choose_delear_title }}</div>
          <div class="choose-delear-desc">{{ section.settings.choose_delear_desc }}</div>
        </div>
      </div>
    </div>
    <div class="delear-stay-title">{{ section.settings.delear_bigtitle3 }}</div>
    <div class="delear-info">{{ section.settings.delear_stay_desc }}</div>
    <div class="delear-map-email-content">
      {%- form 'customer', id: 'footer-newsletter', class: 'Footer__Newsletter Form delear-map-email' -%}
        {% assign shwoTips = false %} 
        {%- if form.posted_successfully? -%}
          {% assign shwoTips = true %}
        {%- else -%}
          {% assign shwoTips = false %} 
        {%- endif -%}
          <input type="email" name="contact[email]" class="Form__Input delear-map-input" aria-label="Enter Email Address" placeholder="Enter Email Address" required>
          <button type="submit" onclick="handleEmail()" class="Form__Submit Button Button--primary">{{ section.settings.delear_submit }}</button>
      {%- endform -%}
      {% comment %} <input class="delear-map-input" placeholder="Enter Email Address" type="text" />
      <div class="submit-btn">{{ section.settings.delear_submit }}</div> {% endcomment %}
    </div>
    {%- if shwoTips == true -%}
      <div class="delear-thanks-main">
        <div class="delear-icon">
          {% assign delear-map-icon = section.settings.delear-map-icon | img_url: '' %}
          <img
            class="delear-map-icon"
            src="{{ delear-map-icon }}"
            loading="lazy"
            alt="{{ section.settings.delear-map-icon.alt | escape }}"
            sizes="auto"
          >
        </div>
        <div class="delear-thanks-text">{{ section.settings.delear_text }}</div>
      </div>
    {%- endif -%}
  </div>
  <div class="delear-exercise-content">
    <div class="delear-exercise-img">
      {% assign delear-exercise-image = section.settings.delear-exercise-image | img_url: '' %}
      <img
        class="delear-exercise-image"
        src="{{ delear-exercise-image }}"
        alt="{{ section.settings.delear-exercise-image.alt | escape }}"
        loading="lazy"
        sizes="auto"
      >
    </div>
  </div>
</div>
