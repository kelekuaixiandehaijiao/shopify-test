    }
    $('.delear-tabs-title').click(function(){
      if($(this).attr('data-delear-status') == '0'){
        $('.delear-map-main').css('display','block')
        $('.delear-exercise-content').css('display','none')
      }else{
        $('.delear-map-main').css('display','none')
        $('.delear-exercise-content').css('display','block')
      }
      console.log()
      $(this).addClass('delear-selected-title')
      $(this).siblings().removeClass('delear-selected-title')
    })
    const config = {
      "api_key": "At_9Xi-7pYhb_kATIXRtLf48OcGcRgZt_cbEX2JSOZw",
      "search_type": "locator",
      "tags": "",
      "title": "Find an E-Bike Dealer",
      "search_text": "Search for E-Bike Dealers within",
      "map_style": "standard",
      "primary_button": "#000000",
      "primary_button_hover": "#484746",
      "secondary_button": "#ffffff",
      "secondary_button_hover": "#6c757d",
      "selected_button": "#000000",
      "opaque": ""
    };

    const qs = Object.keys(config).reduce(function(a, k) {
      a.push(encodeURIComponent(k) + "=" + encodeURIComponent(config[k]));
      return a
