                    </svg>
                </div>
                {%- for block in section.blocks -%}
                    {%- if block.type == 'product_kinds'  -%}
                        {%- assign product_main_color = block.settings.product_main -%}
                        {%- capture show_product -%}
                            {%- render 'customer-price-control', type:'showProduct', cpc_product: product_main_color -%}
                        {%- endcapture -%}
                        {%- if show_product == 'true' -%}
                            <div class="js-fbcu-hide js-fbcu-show{{block.settings.product_slect_type}}" data-index="{{ forloop.index }}" >
                                <div class="fbcu-main-title">
                                    <a class="js-goproduct-page" href="{{ block.settings.fpc_goback_url }}">
                                        <svg   xmlns="http://www.w3.org/2000/svg" width="9" height="18" viewBox="0 0 9 18" fill="none">
                                            <path d="M8.375 1.25L0.978553 8.64645C0.783291 8.84171 0.783291 9.15829 0.978553 9.35355L8.375 16.75" stroke="#333333" stroke-linecap="round"/>
                                       </svg>
                                    </a>
                                 
                                    <span>{{ block.settings.fpc_cus_name }}</span>
                                </div>
                                <div class="fbcu-main-img">
                                    {%- assign product_main_img = block.settings.product_main -%}
                                    {% assign main_image_url =  block.settings.fpc_main_img | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                                    {%- assign pinterest_image = product_main_img.featured_media | img_url: '600x' | prepend: 'https:' -%}
                                    {%- if block.settings.fpc_main_img -%}
                                        <img class="Image--lazyLoad Image--fadeIn" data-src="{{ main_image_url }}"  data-sizes="auto"
                                            alt="{{ block.settings.fpc_main_img.alt }}"
                                        >
                                    {%- else -%}
                                        <img class="Image--lazyLoad Image--fadeIn" data-src="{{ pinterest_image }}"  data-sizes="auto">      
                                    {%- endif -%}
                                   
                                    <span class="Image__Loader"></span>
                                </div>
                                {%- if block.settings.fpc_dimension_title -%}
                                    <div class="fbcu-dimens">
                                        <span class="js-open-dimessdilaog" data-type="{{ block.settings.product_slect_type }}">{{ block.settings.fpc_dimension_title }}</span>
                                    </div>
                                {%- endif -%}
                            </div>
                        {%- endif -%}
                    {%- endif -%}
                {%- endfor -%} 
                <div class="cs-mobshow-pchide cs-render-bikeservice">
                    {%- render 'customer-shop-item', section: section, type: 'bike-service' -%}
                </div>
            </div>
            <div class="fbcu-block-right">
                {%- for block in section.blocks -%}
                    {%- if block.type == 'product_kinds'  -%}
                        {%- assign product_main_title = block.settings.product_main -%}
                        <div class="js-fbcu-hide js-fbcu-show{{ block.settings.product_slect_type }}">
                            <p class="fbcur-product-name">{{ product_main_title.title }}</p>
                        </div>
                    {%- endif -%}
                    {%- if block.type == 'bike_discount_desc' -%}
                        <div class="fbcur-discount-desc">
                            <div class="fbcur-discount-p">
                                {%- if country_code_var == 'gb' -%}
                                    {{ block.settings.gb_fpc_desc_disount}}
                                {%- elsif country_code_var == 'au' -%}
                                    {{ block.settings.au_fpc_desc_disount}}
                                {%- else -%}
                                    {{ block.settings.fpc_desc_disount }}
                                {%- endif -%}
                            </div>
                        </div>
                    {%- endif -%}
                {%- endfor -%} 
                {{ choose_you_color }}
                {%- assign fpc_deliver_to = section.settings.fpc_deliver_to | strip_html -%}
                {%- if fpc_deliver_to != blank -%}
                    <div class="fbcur-deliver-to">
                        <div class="fbcur-choose-title">
                            {{ section.settings.fpc_deliver_to }}
                        </div>
                        {%- render 'customer-shop-item', section: section, type: 'deliver_to' -%}
                    </div>
                   
                {%- endif -%}
                {%- if section.settings.fpc_insurance_show -%}
                    <div class="fb-insurance-box">
                        <div id="fbInsuranceshow">
                        </div>
                    </div>
                   
                {%- endif -%}
       
                {%- for block in section.blocks -%}
                    {%- if block.type == 'product_kinds'  -%}
                      {%- if country_code_var == 'gb' -%}
                        {%- if block.settings.fpc_gift_show_gb -%}
                          {%- render 'customer-shop-item', section: section, block: block, type: 'customer_gift' -%}
                        {%- endif -%}
                      {%- elsif country_code_var == 'au' -%}
                        {%- if block.settings.fpc_gift_show_au -%}
                          {%- render 'customer-shop-item', section: section, block: block, type: 'customer_gift' -%}
                        {%- endif -%}
                      {%- else -%}
                        {%- if block.settings.fpc_gift_show -%}
                          {%- render 'customer-shop-item', section: section, block: block, type: 'customer_gift' -%}
                        {%- endif -%}
                      {%- endif -%}
                    {%- endif -%}
                {%- endfor -%} 

                {%- render 'customer-shop-item', section: section, type: 'customer_service' -%}
                {%- if section.settings.fpc_introduce_show -%}
                    {%- render 'customer-shop-item', section: section, type: 'intro' -%}
                {%- endif -%}
            </div>
            
        </div>
        <div class="fbcu-flex-bottom-pc">
            <div class="fbcubt-content">
                <div class="fbcubt-content-left">
                    {%- render 'customer-shop-item', section: section, type: 'bike-service' -%}
                </div>
                <div class="fbcubt-content-right">
                    {%- render 'customer-shop-item', section: section, type: 'sub-total' -%}
                </div>
            </div>
        </div>
        {% comment %} <div class="cs-mobshow-pchide cs-mobshow-shopfoot">
            {%- render 'customer-shop-item', section: section, type: 'sub-total' -%}
        </div> {% endcomment %}
    </div>
    
</section>
