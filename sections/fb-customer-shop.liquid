{%- assign country_code_var = localization.country.iso_code | downcase -%}
<section id="section-{{ section.id }}"  class="fb-customer-shop">
         {% comment %} only one id or class {% endcomment %}
         <button id="xbike-add-to-cart-main"
            data-variantid="" 
            data-price=""
            data-title=""
            data-gourl=""
            class="js-keep-productdata"
            style="display: none;"
        > 
        </button>
        <div class="js-open-minicart" 
            data-action="open-drawer" 
            data-drawer-id="sidebar-cart" 
            aria-expanded="false" 
            aria-label="Open cart"
            style="display: none;"
        >
        </div>
    {%- capture choose_you_color -%}
        <div class="fbcur-choose-color">
            <p class="fbcur-choose-title">Choose your color</p>
            <ul class="fbcur-choose-ul">
                {%- for block in section.blocks -%}
                    {%- if block.type == 'product_kinds'  -%}
                        {%- assign product_main_color = block.settings.product_main -%}
                        {%- capture show_product -%}
                            {%- render 'customer-price-control', type:'showProduct', cpc_product: product_main_color -%}
                        {%- endcapture -%}
                        {%- if show_product == 'true' -%}
                            {%- capture gb_show -%}
                                {%- render 'customer-price-control', type:'gbShow', cpc_product: product_main_color -%}
                            {%- endcapture -%}
                            {%- capture ca_show -%}
                                {%- render 'customer-price-control', type:'caShow', cpc_product: product_main_color -%}
                            {%- endcapture -%}
                            {%- capture au_show -%}
                                {%- render 'customer-price-control', type:'auShow', cpc_product: product_main_color -%}
                            {%- endcapture -%}
                            <li class="fbcur-choose-item js-fbchoose-productcolor js-choose-item{{ block.settings.product_slect_type }} {% if block.settings.product_slect_type =='1' %}fbcur-choose-item-checked{% endif %} " data-product-id="{{ product_main_color.id }}"
                                data-variantid="{{product_main_color.selected_or_first_available_variant.id}}" 
                                data-price="{{product_main_color.price | money}}"
                                data-type="{{ block.settings.product_slect_type }}"
                                data-title="{{ product_main_color.title }}"
                                data-gbshow="{{ gb_show }}"
                                data-cashow="{{ ca_show }}"
                                data-aushow="{{ au_show }}"
                            >
                                {% assign select_image_select =  block.settings.fpc_color_select_img | img_url: '' %}
                                <img class="fbcur-choose-imgdefalut" src="{{ select_image_select }}" 
                                    alt="{{ block.settings.fpc_color_select_img.alt }}"
                                />
                                {% assign check_image_select =  block.settings.fpc_color_checked_img | img_url: '' %}
                                <img class="fbcur-choose-imgcheck" src="{{ check_image_select }}"  
                                    alt="{{ block.settings.fpc_color_checked_img.alt }}"
                                >
                                <div class="fbcur-choose-product-name">
                                    {{ block.settings.fpc_color_name }}
                                </div>
                            </li>
                        {%- endif -%}
                    {%- endif -%}
                {%- endfor -%}  
            </ul>
        </div>
    {%- endcapture -%}
    <div class="fbcu-content">
        <div class="fbcu-flex-content">
            <div class="fbcu-block-left">
                <div class="fbcu-cusomer-kefu" onclick="TidioShowFun()">
                    <div class="fbcu-kefu-text">
                        <span class="span1">Any questions?</span>
                        <span class="span2">Chat with a freebeat specialist</span>
                    </div>
