{% schema %}
{
    "name": "FbAnimatioBoom",
    "presets": [
        {
            "name": "fb-animation-boom",
            "category": "Fb Sections",
            "blocks": []
        }
        ],
        "settings": [
            {
                "type": "header",
                "content": "section one"
            },
            {
                "type": "richtext",
                "id": "aboom_title_home",
                "label": "Home Title",
                "default": "<p> EXTENDED REALITY </p>"
            },
            {
                "type": "richtext",
                "id": "aboom_title_one",
                "label": "title 1",
                "default": "<p> Immersive Workout </p>"
            },
            {
                "type": "richtext",
                "id": "acboom_desc_one",
                "label": "desc 1",
                "default": "<p>Artist-generated interactive stages turn every class into an immersive workout.</p>"
            },
            {
                "type": "richtext",
                "id": "acboom_title_two",
                "label": "title 2",
                "default": "<p>Rotate for More</p>"
            },
            {
                "type": "richtext",
                "id": "acboom_desc_two",
                "label": "desc 2",
                "default": "<p> Roate the screen to seamlessly transition between a staggering array of cycling, stretching, strength training, and more classes all under one roof. </p>"
            }
    ]
}
{% endschema %}
