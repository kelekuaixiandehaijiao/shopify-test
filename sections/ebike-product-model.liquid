              {%- endif -%}
            </div>
            {%- if section.settings.show_shop_payments -%}
              {% capture pay_item %}mr-2.5 h-6 leading-6{% endcapture %}
              <ul class="ebike-shop-payment fb-flex-x-start py-5 fb-769:py-6">
                {%- for type in shop.enabled_payment_types -%}
                  <li class="ebike-shop-payment-item {{ pay_item }}">{{ type | payment_type_svg_tag }}</li>
                {%- endfor -%}
                <li class="ebike-shop-payment-item {{ pay_item}}">{{ 'klarna' | payment_type_svg_tag }}</li>
              </ul>
            {%- endif -%}
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when 'customerService' -%}
                  {%- render 'ebike-product-model-item', pdptype: 'customer_service', block: block- %}
              {%- endcase -%}
            {%- endfor -%}
            <div class="ebike-width-100 ebike-pcshow-mobhide w-full">
              {%- render 'ebike-product-model-item', pdptype: 'intro', section: section -%}
            </div>
            {%- if section.settings.show_size_fit -%}
              <div class="ebike-size-config w-full">
                {%- render 'ebike-size-config', section: section, type: 'item' -%}
              </div>
            {%- endif -%}
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
