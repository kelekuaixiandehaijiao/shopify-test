          ></div>
          <div class="ebike-fbm-test-container ebike-mob-paddingleft">
            {%- if section.settings.product_build_title != blank -%}
              <div class="ebkie-fbm-bulid ebike-pcshow-mobhide w-full border-b border-e6e6e6 pb-5">
                <span class="text-[18px] font-bold leading-[18px] text-121212">
                  {{- section.settings.product_build_title -}}
                </span>
              </div>
            {%- endif -%}
            {%- render 'ebike-product-form',
              pdptype: 'choose_color',
              section: section,
              single_product: single_product,
              product: product
            -%}
            {%- if section.settings.show_gift -%}
              <div class="ebike-pdp-gift-section w-full py-5 fb-769:py-6">
                <div class="ebike-gift-ul rounded-[10px] border border-e6e6e6 px-5">
                  {%- render 'ebike-product-model-item', pdptype: 'gift', section: section -%}
                </div>
              </div>
            {%- endif -%}
            {%- if section.settings.show_protection -%}
              <div class="ebike-protection w-full fb-769:min-h-[94px] fb-max-768:min-h-[108px]">
                {% comment %}
                  <div class="ebkie-protection-title">
                      {{ section.settings.show_protection_title }}
