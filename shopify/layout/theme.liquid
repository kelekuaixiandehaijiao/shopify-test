    {% render 'extend-configuration' %}
  </head>

  <body class="gradient{% if settings.animations_hover_elements != 'none' %} animate--hover-{{ settings.animations_hover_elements }}{% endif %}">
    {%- if settings.cart_type == 'drawer' and template != 'cart' -%}
      {%- render 'cart-drawer' -%}
    {%- endif -%}
    {% comment %} {% sections 'header-group' %} {% endcomment %}
    <div id="sticky-top-box" class="bg-white">
      {% section 'announcement' %}
      {% section 'header' %}
    </div>
    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>
    <div
      class="fixed-animation-ios lef-0 fixed bottom-0 top-0 z-[-9999] h-[100vh] w-full"
      data-video-src="https://cdn.shopify.com/"
    ></div>
    <div class="PageOverlay"></div>
    {% section 'sidebar-menu' %}
    {% section 'annoucement-offer' %}
    {% sections 'footer-group' %}
    {%- render 'fb-img-video-dialog' -%}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      <li id="a11y-new-window-message">{{ 'accessibility.link_messages.new_window' | t }}</li>
    </ul>

