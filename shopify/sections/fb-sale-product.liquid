}
],
"blocks": [
{
"type": "goods",
"name": "goods",
"settings": [
{
"type": "select",
"id": "introcardtype",
"label": "Bike Type",
"options": [
{
"value": "1",
"label": "lit-bike"
},
{
"value": "2",
"label": "boom-bike"
},
{
"value": "3",
"label": "ebike-high-step"
