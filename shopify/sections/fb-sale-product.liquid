{% schema %}
{
"name": "FbSaleProduct",
"presets": [
{
"name": "fb-sale-product",
"category": "My Sections",
"blocks": []
}
],
"settings": [
{
"type": "text",
"id": "introcardbike",
"label": "Lit Bike",
"default": "Lit Bike"
},
{
"type": "text",
"id": "introcdbike_kinds1",
"label": "Lit Bike Title",
"default": "Exercise Bike"
},
{
"type": "text",
