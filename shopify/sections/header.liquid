<header
  id="section-{{ section.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="header"
  data-section-settings="{{ section_settings }}"
  role="banner"
  class="w-full h-full border-b border-[#dddddd]"
>
  <div class="w-full h-full relative flex justify-between items-center px-5 fb-sm:px-10">
    <div class="Header__LeftItem hidden fb-md:flex justify-start items-center h-full w-auto cursor-pointer">
      <a href="/" class="inline-block w-[92px] h-[22px]">
        <img
          src="{{ section.settings.logo | img_url: '' }}"
          alt="{{ section.settings.logo.alt | escape }}"
          class="w-[92px] h-[22px]"
        >
      </a>
      {%- assign main_menu_handle = section.settings.navigation_menu | default: 'main-menu' -%}
      {%- assign main_menu = linklists[main_menu_handle] -%}
      <nav class="w-auto h-full ml-10">
        <ul class="h-full flex justify-start items-center">
          {%- for link in main_menu.links -%}
            {%- assign linkname = link.title | escape -%}
            <li class="HorizontalList__Item px-6 text-121212 relative h-full fb-flex-xy-center">
              {%- if link.title contains 'SALE' -%}
                {% comment %} sale start {% endcomment %}
                <a href="{{ link.url }}" class="text-14 font-semibold cursor-pointer relative z-1">
                  {% render 'icon' with 'on_sale_icon_small' %} {{- link.title | escape -}}
                </a>
                <div class="Header__LinkSpacer hidden absolute border-b-[2px] border-black bottom-0 left-6 h-0 overflow-hidden">
                  <span class="invisible">{% render 'icon' with 'on_sale_icon_small' %} {{- link.title | escape -}} {{ link.title | escape }}</span>
                </div>
              {%- else -%}
                <a href="{{ link.url }}" class="text-14 font-semibold cursor-pointer relative z-1">
                  {{- link.title | escape -}}
                </a>
                <div class="Header__LinkSpacer hidden absolute border-b-[2px] border-black bottom-0 left-6 h-0 overflow-hidden">
                  <span class="invisible">{{ link.title | escape }}</span>
                </div>
              {%- endif -%}
             
              {% if link.url == '/pages/lit-bike' or linkname == 'BIKES' or link.url == '/products/lit-bike' %}
                {% render 'header-items', section: section %}
              {% else %}
                {% if link.links.size > 0 %}
                  <div
                    class="DropdownMenu py-6 hidden absolute min-w-50 max-w-[270px] bg-white text-left border border-[#dddddd] top-full left-0"
                    aria-hidden="true"
                  >
                    <ul class="Linklist">
                      {%- for sub_link in link.links -%}
                        <li
                          class="Linklist__Item cursor-pointer w-full mb-3 leading-[1.5] text-left"
                          {% if sub_link.links != blank %}
                            aria-haspopup="true"
                          {% endif %}
                        >
                          <a href="{{ sub_link.url }}" class="block px-6 text-[#1C1B22] text-14">
                            {{- sub_link.title | escape }}
                            {% if sub_link.links != blank %}{% render 'icon' with 'select-arrow-right' %}{% endif -%}
                          </a>
                          {% comment %}
                            {%- if sub_link.links != blank -%}
                              <div class="DropdownMenu" aria-hidden="true">
                                <ul class="Linklist">
                                  {%- for sub_sub_link in sub_link.links -%}
                                    <li class="Linklist__Item relative">
                                      <a href="{{ sub_sub_link.url }}" class="Link Link--secondary">{{ sub_sub_link.title | escape }}</a>
                                    </li>
                                  {%- endfor -%}
                                </ul>
                              </div>
                            {%- endif -%}
                          {% endcomment %}
                        </li>
                      {%- endfor -%}
                    </ul>
                  </div>
                {% endif %}
              {%- endif -%}
            </li>
          {%- endfor -%}
        </ul>
      </nav>
    </div>
    {% comment %} mob {% endcomment %}
    <button
      class="w-5 h-5 text-center leading-5 fb-md:hidden cursor-pointer"
      data-action="open-drawer"
      data-drawer-id="sidebar-menu"
